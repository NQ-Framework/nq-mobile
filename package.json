{
  "name": "nq-mobile",
  "version": "0.3.4",
  "author": "NQ Framework",
  "homepage": "https://nqframework.dev/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "build:android": "cross-env ANDROID_APP_VERSION_NAME=$(git describe) ANDROID_APP_VERSION_CODE=$(git rev-list --count HEAD) ./android/gradlew build -b ./android/build.gradle",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:ci && npm run lint",
    "preversion": "npm run lint && npm run test:ci",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "locale": "npm run build && npm run locale:extract",
    "locale:extract": "locl extract -s './www/*.js' -o './rs.json'"
  },
  "private": true,
  "resolutions": {
    "@babel/preset-env": "^7.8.7"
  },
  "dependencies": {
    "@angular/common": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.3",
    "@angular/localize": "^9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@capacitor/android": "^2.0.1",
    "@capacitor/core": "^2.0.1",
    "@ionic-native/core": "^5.24.0",
    "@ionic-native/splash-screen": "^5.24.0",
    "@ionic-native/status-bar": "^5.24.0",
    "@ionic/angular": "^5.0.7",
    "@locl/core": "^1.0.0-rc.1",
    "core-js": "^3.6.5",
    "firebase": "^7.14.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@capacitor/cli": "^2.0.1",
    "@ionic/angular-toolkit": "^2.2.0",
    "@locl/cli": "0.0.1-beta.7",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.2",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "firebase-tools": "^8.1.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "npm-force-resolutions": "0.0.3",
    "protractor": "^5.4.4",
    "ts-node": "~8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "description": "Mobile client for the NQ Framework"
}
