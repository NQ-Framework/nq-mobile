{
  "name": "nq-mobile",
  "version": "0.4.12",
  "author": "NQ Framework",
  "homepage": "https://nqframework.dev/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "build:android": "cross-env ANDROID_APP_VERSION_NAME=$(git describe) ANDROID_APP_VERSION_CODE=$(git rev-list --count HEAD) ./android/gradlew build -b ./android/build.gradle",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:ci && npm run lint",
    "preversion": "npm run lint && npm run test:ci",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "locale": "npm run build && npm run locale:extract",
    "locale:extract": "locl extract -s './www/*.js' -o './rs.json'"
  },
  "private": true,
  "resolutions": {
    "@babel/preset-env": "^7.8.7"
  },
  "dependencies": {
    "@angular/common": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/fire": "^6.0.3",
    "@angular/forms": "^10.2.0",
    "@angular/localize": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@capacitor/android": "^2.4.2",
    "@capacitor/core": "^2.4.2",
    "@ionic-native/core": "^5.29.0",
    "@ionic-native/splash-screen": "^5.29.0",
    "@ionic-native/status-bar": "^5.29.0",
    "@ionic/angular": "^5.4.1",
    "@locl/core": "^1.0.1",
    "capacitor-fcm": "^2.0.0",
    "core-js": "^3.6.5",
    "firebase": "^7.24.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@capacitor/cli": "^2.4.2",
    "@ionic/angular-toolkit": "^2.3.3",
    "@locl/cli": "0.0.1-beta.9",
    "@types/jasmine": "^3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.28",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "firebase-tools": "^8.13.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-force-resolutions": "0.0.3",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.8.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "description": "Mobile client for the NQ Framework"
}
